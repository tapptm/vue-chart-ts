derColor: \"rgba(17, 21, 85, 0.777)\",\n            backgroundColor: \"rgba(17, 21, 85, 0.377)\",\n            fill: \"start\",\n            pointBackgroundColor: \"rgba(17, 21, 85, 0.800)\",\n            data: [3000, 1000, 2000, 3000]\n        },\n        {\n            label: \"Expenses\",\n            borderColor: \"rgba(180, 38, 20, 0.766)\",\n            backgroundColor: \"rgba(180, 38, 20, 0.366)\",\n            fill: \"origin\",\n            pointBackgroundColor: \"rgba(180, 38, 20, 0.800)\",\n            data: [1200, 1400, 2600, 1000]\n        }\n    ]\n};\n\nconst type = \"linear\";\n\nexport const options = {\n    responsive: true,\n    maintainAspectRatio: false,\n    title: {\n      display: true,\n      text: \"new - \" + type\n    },\n    tooltips: {\n      mode: \"index\",\n      intersect: false,\n      callbacks: {\n        footer(tooltipItems: any, data:any) {\n          let sum = 0;\n          tooltipItems.forEach((tooltipItems:any) => {\n            sum +=\n              data.datasets[tooltipItems.datasetIndex].data[tooltipItems.index];\n          });\n          return \"sum : \" + sum;\n        }\n      }\n    },\n    scales: {\n      xAxes: [\n        {\n          type: \"time\",\n          time: {\n            parser: timeFormat,\n            tooltipFormat: \"ll HH:mm\",\n            unit: \"day\"\n          },\n          display: true,\n          scaleLabel: {\n            display: true,\n            labelString: \"Date\"\n          },\n          ticks: {\n            display: true,\n            maxRotation: 90,\n            max: moment(end).format(timeFormat),\n            min: moment(start).format(timeFormat)\n          }\n        }\n      ],\n      yAxes: [\n        {\n          display: true,\n          scaleLabel: {\n            display: true,\n            labelString: \"Costs\"\n          }\n        }\n      ]\n    },\n    layout: {\n      padding: {\n        left: 20\n      }\n    }\n  };\n"],"sourceRoot":""}]}